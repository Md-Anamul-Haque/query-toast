{
  "version": "0.1.5",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "query-toast",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Md-Anamul-Haque/query-toast.git"
  },
  "bugs": {
    "url": "https://github.com/Md-Anamul-Haque/query-toast/issues"
  },
  "homepage": "https://github.com/Md-Anamul-Haque/query-toast#readme",
  "keywords": [
    "react-toastify",
    "axios"
  ],
  "author": "md-anamul-haque",
  "module": "dist/query-toast.esm.js",
  "size-limit": [
    {
      "path": "dist/query-toast.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/query-toast.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "husky": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "react-toastify": "^9.1.3",
    "sweetalert2": "^11.7.12"
  }
}
